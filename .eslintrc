{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    // Adicione ou ajuste regras conforme necess√°rio
    "prettier/prettier": "error",
    "no-unused-vars": "off"
  }
}
